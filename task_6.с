#include <stdio.h>
#include <stdlib.h>
int main()
{
    int m,n;
    printf("Type number m \n");
    scanf("%d", &m);
    printf("Type number n \n");
    scanf("%d", &n);
    if(m>n){
        printf("Number m > n");
    } else if(n>m){
        printf("Number m < n");
    } else{
        printf("The numbers are equal");
    }
}
